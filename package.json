{
  "name": "genui-next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./configuration/webpack.config.js --hot --mode=development",
    "build:static": "webpack --config ./configuration/webpack.config.js --mode=production",
    "build:analyze": "ANALYZE=true yarn build",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "test": "yarn test:unit && yarn test:e2e",
    "lint": "next lint",
    "prettier": "prettier --write ./",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "hooks-for-redux": "^2.0.3",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.2.2",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "remove": "^0.1.5",
    "styled-components": "^5.3.3",
    "styled-normalize": "^8.0.7",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@next/bundle-analyzer": "^12.1.0",
    "@playwright/test": "^1.19.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.19",
    "@types/styled-components": "^5.1.24",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.6",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --cache --fix --max-warnings 0",
      "prettier --write ./"
    ]
  }
